        "wpackagist-theme/hueman":"*",
        "wpackagist-plugin/restaurant-menu-using-woocommerce": "^4.3"
    },
    "extra": {
        "installer-paths": {
            "your/custom/path/{$name}/": ["vendor:my_organization"]
        }
    }
}
